{
  "name": "./",
  "module": "server.ts",
  "type": "module",
  "scripts": {
    "start": "bun run src/server.ts",
    "dev": "bun run --watch src/server.ts",
    "build": "bun build src/server.ts --outdir dist --target node",
    "lint": "bun run eslint src/**/*.ts",
    "format": "bun run prettier --write src/**/*.ts",
    "test": "bun run vitest"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/json2csv": "^5.0.7",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^22.7.8",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2"
  },
  "peerDependencies": {
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.25.9",
    "@babel/preset-env": "^7.25.9",
    "@babel/preset-typescript": "^7.25.9",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "json2csv": "^6.0.0-alpha.2",
    "mongoose": "^8.7.2"
  }
}
